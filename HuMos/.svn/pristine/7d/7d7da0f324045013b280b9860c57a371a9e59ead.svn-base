<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical" >

    <include
        android:id="@+id/feedback_one_view"
        layout="@layout/head_layout" />

    <ListView
        android:id="@+id/lv_feedbackdetails"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="1.0"
        android:cacheColorHint="#00000000"
        android:divider="#f5f5f5"
        android:listSelector="@color/transparent"
        android:scrollbars="none" >

    </ListView>

    <TextView
        android:id="@+id/tv_question_remind"
        style="@style/size_12_gray008"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#F5F5F5"
        android:padding="10dp"
        android:text="温馨提示：酒斯基平台具有代驾、 寻酒等服务功能，具有行酒令、玩骰子等休闲娱乐功能，您如果有好的意见和建议请给我们留言，您的建议一经采纳必有礼品赠送！酒斯基是酒友永远的后盾，祝您用酒愉快！" />

</LinearLayout>